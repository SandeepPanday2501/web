@model List<WebApplication1.Models.student>



@{
    ViewBag.Title = "view";
    int sn = 0;
}
<a href="@Url.Action("Create")">Add new record</a>
<style>
    table, th, td {
        border: 2px solid;
        background-color: black;
        font-size: 12px;
        color: white;
    }

    form {
        width: 100px;
        display: inline-block;
    }

    input[type=submit] {
        color: #9a9298;
    }
</style>

<h2>Welcome</h2>
<p>Table Data</p>

<table class=" table" style="border:2px dashed blue;">
    <tr>
        <th>StudentID</th>
        <th>StudentName</th>
        

        @*
            <tr>
            <td>1</td>
            <td>Shiroyasha</td>
            <td>Kathmandu</td>
            </tr>*@
        @foreach (var data in Model)
        {
        <tr>

            <td>@(++sn)</td>
            <td>@(data.StudentName)</td>
        

        </tr>
    }

    </table>

    <style type="text/css">
        label, input {
            margin-top: 2px;
        }

        label {
            margin-right: 2px;
        }
    </style>
 

    


